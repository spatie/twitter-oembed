{
    "name": "spatie/twitter-oembed",
    "description": "Retrieve tweets with the Twitter oEmbed API",
    "keywords": [
        "spatie",
        "twitter-oembed"
    ],
    "homepage": "https://github.com/spatie/twitter-oembed",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "symfony/css-selector": "^5.3",
        "symfony/dom-crawler": "^5.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^v3.0",
        "pestphp/pest": "^1.20",
        "pestphp/pest-plugin-parallel": "^0.3.1",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\TwitterOEmbed\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest --parallel",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
